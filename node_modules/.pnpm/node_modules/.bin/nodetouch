#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/touch@3.1.0/node_modules/touch/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/touch@3.1.0/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/node_modules:/Users/maciek/Work/dotbim-ts/node_modules:/Users/maciek/Work/node_modules:/Users/maciek/node_modules:/Users/node_modules:/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/node_modules/touch/bin/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/node_modules/touch/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/touch@3.1.0/node_modules/touch/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/touch@3.1.0/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/node_modules:/Users/maciek/Work/dotbim-ts/node_modules:/Users/maciek/Work/node_modules:/Users/maciek/node_modules:/Users/node_modules:/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/node_modules/touch/bin/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/node_modules/touch/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
