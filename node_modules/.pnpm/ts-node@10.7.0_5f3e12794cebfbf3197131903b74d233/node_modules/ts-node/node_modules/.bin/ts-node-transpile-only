#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/ts-node@10.7.0_5f3e12794cebfbf3197131903b74d233/node_modules/ts-node/dist/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/ts-node@10.7.0_5f3e12794cebfbf3197131903b74d233/node_modules/ts-node/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/ts-node@10.7.0_5f3e12794cebfbf3197131903b74d233/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/node_modules:/Users/maciek/Work/dotbim-ts/node_modules:/Users/maciek/Work/node_modules:/Users/maciek/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/ts-node@10.7.0_5f3e12794cebfbf3197131903b74d233/node_modules/ts-node/dist/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/ts-node@10.7.0_5f3e12794cebfbf3197131903b74d233/node_modules/ts-node/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/ts-node@10.7.0_5f3e12794cebfbf3197131903b74d233/node_modules:/Users/maciek/Work/dotbim-ts/node_modules/.pnpm/node_modules:/Users/maciek/Work/dotbim-ts/node_modules:/Users/maciek/Work/node_modules:/Users/maciek/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin-transpile.js" "$@"
else
  exec node  "$basedir/../../dist/bin-transpile.js" "$@"
fi
